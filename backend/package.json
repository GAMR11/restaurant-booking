{
  "name": "restaurant-backend",
  "version": "1.0.0",
  
    "scripts": {
      "dev": "nodemon src/index.ts",
      "build": "tsc",
      "start": "node dist/index.js",
      "prisma:generate": "prisma generate",
      "prisma:migrate": "prisma migrate dev",
      "prisma:studio": "prisma studio",
      "seed": "node dist/prisma/seed.js",
      "seed:dev": "ts-node prisma/seed.ts",
      "setup-db": "npx prisma db push --skip-generate && npm run seed",
      "deploy": "npx prisma migrate deploy && npx prisma generate && npm run seed && npm start",
      "railway:start": "npx prisma migrate deploy && node dist/index.js"
    },
  
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^128.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.9",
    "nodemon": "^3.0.2",
    "prisma": "^6.19.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
